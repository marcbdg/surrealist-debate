        <div class="header-container">
            <header class="wrapper clearfix">
                <h1 class="title">Surrealist Debate</h1>
                <!-- <nav>
                    <ul>
                        <li><a href="#">nav ul li a</a></li>
                        <li><a href="#">nav ul li a</a></li>
                        <li><a href="#">nav ul li a</a></li>
                    </ul>
                </nav> -->
            </header>
        </div>

        <div class="main-container">
            <div class="main wrapper clearfix">

                <article>
                    <header>
                        <div style="border: 2px dotted #333; float: right; padding: 20px; background: #ccc">
                        	<b>What's Next:</b>
                        	<ul style="margin:0;">
                        		<li>populate next round when selecting any winner</li>
                        	</ul>
                        </div>


                        <h1>Can you dig it?</h1>
                        <p>
                           <input type="button" value="64 topics" onclick="setupGame(64)" />
                           <input type="button" value="32 topics" onclick="setupGame(32)" />
                           <input type="button" value="16 topics" onclick="setupGame(16)" />
                           <input type="button" value="8 topics" onclick="setupGame(8)" />
						   |
						   <input id="startGameButton" type="button" value="Start Game" />
                        </p>
                    </header>
                    <section>
                       <h2>Here ya go, <span id="numTopics"></span> topics:</h2>

                       <div id="brackets">

                          <div class="division division-upper clearfix">
                             <div class="bracket bracket-1">
                                <div>bracket 1</div>
                             </div>
							 <div class="round round-semi">
								 Semifinals
							 	<div class="match"></div>
							 </div>
                             <div class="bracket bracket-3">
                                <div>bracket 3</div>
                             </div>
                          </div>

                          <div class="round round-final">
							  Finals
							  <div class="match"></div>
                          </div>

                          <div class="division division-lower clearfix">
                             <div class="bracket bracket-2">
                                <div>bracket 2</div>
                             </div>

							 <div class="round round-semi">
								 Semifinals
							 	<div class="match"></div>
							 </div>

                             <div class="bracket bracket-4">
                                <div>bracket 4</div>
                             </div>
                          </div>

                       </div>

                    </section>
                    <footer>
                        <!-- <h3>Moves: <span id="totalMoves">0</span> <span class="lastMove"></span></h3> -->
                    </footer>
                </article>

                <!-- <aside>
                    <h3>
                       Goal
                    </h3>
                    <table id="goal"></table>
                    <input type="checkbox" id="goalGridCheckbox" /> <label for="goalGridCheckbox">Show pixels</label>
                </aside> -->

            </div> <!-- #main -->
        </div> <!-- #main-container -->

        <div class="footer-container">
            <footer class="wrapper">
                <!-- <p>(testing)</p>
                <p>
                   <input type="checkbox" id="foldingMode" /> <label for="foldingMode">Folding mode on?</label>
                </p> -->
            </footer>
        </div>

        <!-- Templates -->
        <div id="pixelTemplate" class="htmlTemplate"><div class="pixel"></div></div>

